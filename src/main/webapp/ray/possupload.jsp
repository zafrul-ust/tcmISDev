<title>
<fmt:message key="label.uploadfile"/>
</title>

<script language="JavaScript" type="text/javascript">
<!--
//add all the javascript messages here, this for internationalization of client side javascript messages
var messagesData = new Array();
messagesData={alert:"<fmt:message key="label.alert"/>",and:"<fmt:message key="label.and"/>",
file:"<fmt:message key="label.file"/>",filetypenotallowed:"<fmt:message key="label.filetypenotallowed"/>",
validvalues:"<fmt:message key="label.validvalues"/>",submitOnlyOnce:"<fmt:message key="label.submitOnlyOnce"/>"};

function checkInput(){
	$("errorMessagesArea").innerHTML = "";

	if($v("theFile").length == 0) {
		alert(messagesData.validvalues+messagesData.file);
		return false;
	} else if (!validateFileExtension()) {
		return false;
	} else
		return true;
	
}

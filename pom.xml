  <packaging>war</packaging>

  <scm>
    <connection>scm:svn:https://svn.tcmis.com/prd/cappuccino/tcmis/trunk</connection>
    <developerConnection>scm:svn:https://svn.tcmis.com/prd/cappuccino/tcmis/branches/dev</developerConnection>
    <url>https://svn.tcmis.com/prd/cappuccino/tcmis/</url>
  </scm>

  <build>
    <finalName>tcmIS</finalName>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>buildnumber-maven-plugin</artifactId>
        <version>1.4</version>
        <executions>
          <execution>
            <phase>validate</phase>
            <goals>
              <goal>create</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <doCheck>false</doCheck>
          <doUpdate>false</doUpdate>
          <providerImplementations>
            <svn>javasvn</svn>
          </providerImplementations>
        </configuration>
        <dependencies>
          <dependency>
            <groupId>com.google.code.maven-scm-provider-svnjava</groupId>
            <artifactId>maven-scm-provider-svnjava</artifactId>
            <version>2.0.2</version>
          </dependency>
        </dependencies>
      </plugin>
      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.8.2</version>
        <configuration>
          <skip>true</skip>
        </configuration>
        <executions>
          <execution>
            <id>deploy-tarball</id>
            <phase>deploy</phase>
            <goals>
              <goal>deploy-file</goal>
            </goals>
            <configuration>
              <skip>false</skip>
              <file>${project.build.directory}/${project.artifactId}-${project.version}.tar</file>
              <groupId>${project.groupId}</groupId>
              <artifactId>${project.artifactId}</artifactId>
              <version>${project.version}</version>
              <repositoryId>${deployFileRepositoryId}</repositoryId>
              <url>${deployFileUrl}</url>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <compilerArguments>
            <bootclasspath>${java.home}/lib/rt.jar${path.separator}${java.home}/lib/jce.jar${path.separator}${java.home}/lib/jsse.jar</bootclasspath>
          </compilerArguments>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <version>3.1.1</version>
        <executions>
          <execution>
            <id>copy</id>
            <phase>package</phase>
            <goals>
              <goal>copy</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <artifactItems>
            <artifactItem>
              <groupId>com.tcmis</groupId>
              <artifactId>labelprinter</artifactId>
              <version>V_20191005</version>
              <type>jar</type>
              <outputDirectory>${project.build.directory}/html/labelPrinterJar</outputDirectory>
              <destFileName>LabelPrinter.jar</destFileName>
            </artifactItem>
            <artifactItem>
              <groupId>com.borland</groupId>
  		      <artifactId>jbcl</artifactId>
  		      <version>004.000.117.000</version>
              <type>jar</type>
              <outputDirectory>${project.build.directory}/html/labelPrinterJar</outputDirectory>
              <destFileName>jbcl3.0.jar</destFileName>
            </artifactItem>
            <artifactItem>
              <groupId>com.sun.pdfview</groupId>
    	      <artifactId>PDFRenderer</artifactId>
    	      <version>0.9.1</version>
              <type>jar</type>
              <outputDirectory>${project.build.directory}/html/labelPrinterJar</outputDirectory>
            </artifactItem>
          </artifactItems>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <version>1.7</version>
        <executions>
          <execution>
            <id>generate-version-js-file</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <ant antfile="build.xml" target="generate-version-js-file"/>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>copy-report-jocl-files</id>
            <phase>package</phase>
             <goals>
                <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <ant antfile="build.xml" target="copy-report-jocl-files">
                   <property name="buildName" value="${project.build.finalName}"/>
                 </ant>
               </target>
             </configuration>
           </execution>
           <execution>
             <id>sign-jars</id>
             <phase>package</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <ant antfile="build.xml" target="sign-jars"/>
               </target>
             </configuration>
           </execution>
           <!--<execution>
             <id>create-tarball</id>
             <phase>package</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <ant antfile="build.xml" target="create-tarball"/>
               </target>
             </configuration>
           </execution>-->
        </executions>
      </plugin>
      <plugin>
 		<groupId>org.apache.maven.plugins</groupId>
	    <artifactId>maven-assembly-plugin</artifactId>
  		<configuration>
   		  <descriptor>src/assembly/bin.xml</descriptor>
          <finalName>${project.artifactId}-${project.version}</finalName>
		  <tarLongFileMode>gnu</tarLongFileMode>
		  <attach>false</attach>
        </configuration>
        <executions>
          <execution>
            <id>create-tarball</id>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <filtering>true</filtering>
        <includes>
          <include>**/*.properties</include>
        </includes>
      </resource>
      <resource>
        <directory>src/main/resources</directory>
        <filtering>false</filtering>
        <excludes>
          <exclude>**/*.properties</exclude>
        </excludes>
      </resource>
    </resources>
  </build>

  <profiles>
    <profile>
      <id>qa</id>
      <properties>
        <deployFileUrl>${project.distributionManagement.snapshotRepository.url}</deployFileUrl>
        <deployFileRepositoryId>${project.distributionManagement.snapshotRepository.id}</deployFileRepositoryId>
      </properties>
    </profile>
  </profiles>

  <properties>

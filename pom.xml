    <deployFileUrl>${project.distributionManagement.repository.url}</deployFileUrl>
    <deployFileRepositoryId>${project.distributionManagement.repository.id}</deployFileRepositoryId>
    <tcmis.build.timestamp>${maven.build.timestamp}</tcmis.build.timestamp>
    <maven.build.timestamp.format>yyyy-MM-dd HH:mm</maven.build.timestamp.format>
